<%= form_for(@entry) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.text_area :title, placeholder: "What will be the title?" %>
  </div>
  <div class="field">
    <%= f.text_area :body, placeholder: "Tell us your new story!" %>
  </div>
  <%= f.submit "Post", class: "btn btn-primary" %>
<% end %>